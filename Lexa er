#include <iostream>
#include <vector>
#include <cmath>
#include <fstream>
#include <iomanip>

using namespace std;

int main()
{
	double x, e;
	int k = 2;
	bool f = 0;
	cout << "x:";
	cin >> x;
	cout << "e:";
	cin >> e;
	vector <double> vec;
	vec.push_back(1);
	vec.push_back(x);
	vec.push_back(x * x * x);
	while (abs(vec.at(k) - vec.at(k - 1)) >= e && f == 0)
	{
		k++;
		vec.push_back((12. - vec.at(k - 1) * x) / (vec.at(k - 2) * vec.at(k - 2) + sqrt(abs(((vec.at(k - 3)))))));
		cout << k << " " << vec.at(k) << endl;
		
		if (vec.at(k - 3) < 0 || (vec.at(k - 2) * vec.at(k - 2) + sqrt(abs((vec.at(k - 3))))) == 0)
			f = 1;
		if(f == 0)
			cout << "Znamenatel " << (vec.at(k - 2) * vec.at(k - 2) + sqrt(abs(vec.at(k - 3)))) << endl;
	}
	if (f == 1)
		cout << "ERROR";
	return 0;
}
